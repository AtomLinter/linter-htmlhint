{
  "name": "linter-htmlhint",
  "version": "1.0.3",
  "private": true,
  "description": "A plugin for Atom Linter providing an interface to HTMLHint.",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/AtomLinter/linter-htmlhint.git"
  },
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "scripts": {
    "test": "eslint ."
  },
  "keywords": [
    "html",
    "lint",
    "htmlhint"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AtomLinter/linter-htmlhint/issues"
  },
  "homepage": "https://github.com/AtomLinter/linter-htmlhint#readme",
  "dependencies": {
    "atom-linter": "^4.6.1",
    "atom-package-deps": "^4.0.1",
    "htmlhint": "~0.9.12"
  },
  "devDependencies": {
    "eslint": "^2.4.0",
    "babel-eslint": "^5.0.0",
    "eslint-config-airbnb": "^6.1.0"
  },
  "eslintConfig": {
    "rules": {
      "comma-dangle": [
        2,
        "never"
      ],
      "no-console": 0
    },
    "extends": "airbnb/base",
    "parser": "babel-eslint",
    "globals": {
      "atom": true
    },
    "env": {
      "es6": true,
      "node": true
    }
  },
  "package-deps": [
    "linter"
  ],
  "providedServices": {
    "linter": {
      "versions": {
        "1.0.0": "provideLinter"
      }
    }
  }
}
