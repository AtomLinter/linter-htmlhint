{
  "name": "linter-htmlhint",
  "version": "1.2.1",
  "private": true,
  "description": "A plugin for Atom Linter providing an interface to HTMLHint.",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/AtomLinter/linter-htmlhint.git"
  },
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "scripts": {
    "test": "apm test",
    "lint": "eslint ."
  },
  "keywords": [
    "html",
    "lint",
    "htmlhint"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AtomLinter/linter-htmlhint/issues"
  },
  "homepage": "https://github.com/AtomLinter/linter-htmlhint#readme",
  "dependencies": {
    "atom-linter": "^6.0.0",
    "atom-package-deps": "^4.0.1",
    "htmlhint": "0.9.13",
    "strip-json-comments": "^2.0.1",
    "tiny-promisify": "^0.1.1"
  },
  "devDependencies": {
    "eslint": "^3.0.1",
    "eslint-config-airbnb-base": "^4.0.2",
    "eslint-plugin-import": "^1.10.2"
  },
  "eslintConfig": {
    "rules": {
      "comma-dangle": [
        2,
        "never"
      ],
      "global-require": 0,
      "import/no-unresolved": [
        2,
        {
          "ignore": [
            "atom"
          ]
        }
      ]
    },
    "extends": "airbnb-base",
    "globals": {
      "atom": true
    },
    "env": {
      "es6": true,
      "node": true
    }
  },
  "package-deps": [
    "linter"
  ],
  "providedServices": {
    "linter": {
      "versions": {
        "1.0.0": "provideLinter"
      }
    }
  }
}
